---
import type {Image} from "../types"
import ImageComponent from "./Image.astro"

interface Props {
	images: Image[]
	loading?: HTMLImageElement["loading"]
}

const {images = [], loading = "eager"} = Astro.props
---

<>
	{images.map((props) => (
		<ImageComponent {...props} loading={loading}/>
	))}
</>
